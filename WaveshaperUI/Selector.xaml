<UserControl x:Class="GraphicsWorkForWaveshaper.Selector"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GraphicsWorkForWaveshaper"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="SlateGray" Height="450">
        <TabControl Name="Tabs" Background="Gray" TabStripPlacement="Bottom">
            <TabItem>
                <TabItem.Header>
                    <Button Name="add_effect_button" Content="Add Effect" Height="60" Width="60"></Button>
                </TabItem.Header>
                <Slider></Slider>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
